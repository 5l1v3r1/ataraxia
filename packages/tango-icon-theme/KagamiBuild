# Description: Icon theme that follows the Tango visual guidelines
# URL:         http://tango.freedesktop.org/
# Maintainer:  protonesso, nagakamira at gmail dot com
# Section:     x11

name=tango-icon-theme
version=0.8.90
release=1
options=('~strip')
source=("https://github.com/ataraxialinux/storage/raw/master/tango.tar.xz")
noextract=("tango.tar.xz")

build() {
	cd "$SRC"
	mkdir -p "$PKG"

	bsdtar -C "$PKG" -xvf tango.tar.xz
}
